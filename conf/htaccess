RewriteEngine on
RewriteCond %{HTTP:Authorization} ^(.*)
RewriteRule .* - [e=HTTP_AUTHORIZATION:%1]

RewriteCond  %{DOCUMENT_ROOT}%{REQUEST_URI}    -f [OR]
RewriteCond  %{DOCUMENT_ROOT}%{REQUEST_URI}    -d

RewriteRule  ^(.+) - [L]
RewriteRule  ^(.*)/web/(.*)$   /web/$2 [L]
RewriteRule ^(.*)$  /index.php [L]
